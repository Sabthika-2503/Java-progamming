import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
    Scanner inn =  new Scanner(System.in);
    int num = inn.nextInt();
    int temp = num;
    int sum = 0;

        while (temp > 0) {
            sum = sum + (temp % 10);
            temp = temp / 10;
        }

        if (num % sum == 0)
            System.out.println("Harshad Number");
        else
            System.out.println("Not Harshad Number");
    }
}
